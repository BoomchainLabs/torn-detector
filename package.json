  },
  "homepage": "https://github.com/pcaversaccio/torn-detector#readme",
  "bugs": {
    "url": "https://github.com/pcaversaccio/torn-detector/issues"
  },
  "author": "Pascal Marco Caversaccio <pascal.caversaccio@hotmail.ch>",
  "license": "WTFPL",
  "packageManager": "pnpm@10.19.0",
  "scripts": {
